# Staging Environment Variables
ENVIRONMENT=staging
LOG_LEVEL=DEBUG

# CORS - Allow staging domains
CORS_ORIGINS=https://staging-converter.imrajeshkumar7492.dev

# Security
SECRET_KEY=${SECRET_KEY}

# File Upload Limits
MAX_FILE_SIZE=1073741824
